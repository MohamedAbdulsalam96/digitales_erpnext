 "html": "{% if doc.voucher_type == \"Credit Note\"%}\n<div class=\"print-preview\">\n<div class=\"print-format\">\n<table width=100% style=\"line-height:0.6;\">\n  <tr style=\"text-align:left\";style=\"align:center;\"><td width=70%>\n  <img src=\"files/Digi logo.png\" height=\"80%\" width=\"50%\"></img></td>\n       <td width=60% style=\"text-align:right;\"></td>\n  </tr>\n</table>\n<table width=100% style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n        <tr width=100%>\n             <td width=60%>\n                  <table width=100% style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n                          <tr>\n                               <td width=90% ><h5><b>Digital Education Services Pty. Ltd</b></h5></td>\n                               <td width=10% align=\"left\"><td>\n                          </tr>\n                          <tr>\n                               <td width=40%>192 Gladstone Street South Melbourne VIC 3205<br>Tel: 03 9690 5959 Fax: 039690 5922<br>www.digitales.com.au | info@digitales.com.au<br>ABN 49006791457</td>\n                               <td width=60% align=\"left\">{{ \" \" }}\n                                </td>\n                          </tr>\n\n                  </table>\n             </td>         \n        </tr>\n</table>\n  <div class=\"print-heading\">\n      <h3>CREDIT NOTE </h3>\n       <h4 class=\"text-muted\">#{{doc.name}}</h4>\n  </div>\n<div class=\"row\">\n    <div class=\"col-xs-6\">\n        <div class=\"row\">\n            <div class=\"col-xs-5 text-left\" ><b>Pay To/Recd From</b></div>\n            <div class=\"col-xs-7\">{{ doc.pay_to_recd_from or \"\" }}</div>\n        </div>\n    </div>\n    <div class=\"col-xs-6\">\n        <div class=\"row\">\n            <div class=\"col-xs-5 text-right\"><b>Posting Date</b></div>\n            <div class=\"col-xs-7\">{{ frappe.utils.formatdate(doc.posting_date) or \"\" }}</div>\n        </div>\n    </div>\n</div>\n<hr>\n<table width=100% class=table-bordered>\n    <tr width=100%>\n        <th>Sr</th>\n        <th>Details</th>\n        <th>Credit</th>\n    </tr>\n{% set c=1 %}\n    {%- for row in doc.entries -%}\n    {% if row.credit %}\n    <tr width=100%>\n\t    <td style=\"width:5%;\" align=\"right\">{{ c or \" \" }}</td>\n        <td style=\"width:10%;\" align=\"left\">\n            <!-- check the master type of account if not availble show as it is-->\n{{frappe.db.get_value(\"Account\",row.account,\"master_type\")}}\n            {% if \"GST\" in row.account %}\n                GST\n            {% elif frappe.db.get_value(\"Account\",row.account,\"master_type\") == \"Customer\" %}\n                Goods and Services\n            {% else %}\n                {{ row.account }}\n            {%endif%}\n        </td>\n\t    <td style=\"width:10%;\" align=\"left\">{{ row.get_formatted(\"credit\",row) }}</td>\n    </tr>\n{% set c=c+1%}\n    {% endif %}\n\t{%- endfor -%}\n</table>\n<br>\n<div class=\"row\">\n    <div class=\"col-xs-6\">\n        <div class=\"row\">\n            <div class=\"col-xs-6\" align=\"right\"><b>Total Credit</b></div>\n            <div class=\"col-xs-6\" align=\"right\">{{ doc.get_formatted(\"total_credit\",doc) or 0.0}}</div>\n        </div>\n    </div>\n    <div class=\"col-xs-6\">\n        <div class=\"row\">\n            <div class=\"col-xs-6\" align=\"right\"><b>Reference Number</b></div>\n            <div class=\"col-xs-6\" align=\"right\">{{ doc.cheque_no or \"\"}}</div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-xs-6\" align=\"right\"><b>Reference Date</b></div>\n            <div class=\"col-xs-6\" align=\"right\">{{ doc.cheque_date or \"\"}}</div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-xs-6\" align=\"right\"><b>User Remark</b></div>\n            <div class=\"col-xs-6\" align=\"right\">{{ doc.user_remark or \"\"}}</div>\n        </div>\n    </div>\n</div>\n</div>\n</div>\n{% else %}\n<p align=\"center\"><b>Invalid voucher type</b></p>\n{% endif %}", 
