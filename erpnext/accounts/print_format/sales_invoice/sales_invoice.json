   <div class=\"row\">\n                        <div class=\"col-xs-12\">{{shipTo.country}}.</div>\n                    </div>\n                </div>\n                <div class=\"col-xs-4\">\n                    <div class=\"row\">\n                        <div class=\"col-xs-5\" align=\"right\"><b>PO#</b></div>\n                        <div class=\"col-xs-7\">{{doc.po_no or ''}}</div>\n                    </div>\n                    <div class=\"row\">\n                        <div class=\"col-xs-5\" align=\"right\"><b>Contract#</b></div>\n                        <div class=\"col-xs-7\">{{doc.contract_number or ''}}</div>\n                    </div>\n                    <div class=\"row\">\n                        <div class=\"col-xs-5\" align=\"right\"><b>Budget</b></div>\n                        <div class=\"col-xs-7\">{{doc.budget or ''}}</div>\n                    </div>\n                    <div class=\"row\">\n                        <div class=\"col-xs-5\" align=\"right\" style=\"white-space: nowrap;\"><b>Order Type</b></div>\n                        <div class=\"col-xs-7\">{{doc.new_order_type or ''}}</div>\n                    </div>\n                    <div class=\"row\">\n                        <div class=\"col-xs-5\" align=\"right\"><b>Terms</b></div>\n                        <div class=\"col-xs-7\">{{doc.terms or ''}}</div>\n                    </div>\n                </div>\n            </div>\n\n</br>\n<table width=100% class=table-bordered>\n         <tr width=100%>\n<th>Qty Ordered</th>\n<th>Qty Supplied</th>\n<th>Item Code</th>\n<th>Item Name</th>\n<th>Media</th>\n<th>Unit<br>Price</th>\n<th>Extended<br>Value</th>\n<th>GST<br>Value</th>\n<th>Total<br>Value</th>\n\n\t\t</tr>\n\n\t\t{%- for row in doc.entries -%}\n                     <tr width=100% style=\"white-space:nowrap;\">\n\t\t\t<td style=\"width:3%;\" class=\"text-center\">{{ row.qty or \" \" }}</td>\n\t\t\t<td style=\"width:5%;\" class=\"text-center\">{{ row.delivered_qty or \" \" }}</